{
  "name": "realtime-location-app",
  "version": "1.0.0",
  "description": "Real-time location sharing app with React (Vite) frontend and Express backend",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "client": "cd client && npm run dev",
    "server": "nodemon server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd client && npm install && npm run build",
    "start": "node server/index.js"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cloudinary": "^1.41.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.5.0",
    "multer": "^1.4.5-lts.1",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.3"
  },
  "engines": {
    "node": ">=18"
  }
}
